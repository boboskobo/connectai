APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=info

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=rockrms
DB_USERNAME=root
DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

ROCK_API_KEY=your_rock_api_key
ROCK_BASE_URL=https://your-rock-instance.com/api
ROCK_DEFAULT_CONNECTION_STATUS_ID=1949

# Application
NODE_ENV=development
PORT=3000

# GHL Marketplace App
GHL_APP_CLIENT_ID=your_client_id
GHL_APP_CLIENT_SECRET=your_client_secret
GHL_APP_SSO_KEY=your_sso_key
GHL_WEBHOOK_SECRET=your_webhook_secret
GHL_API_DOMAIN=https://services.leadconnectorhq.com

# Security
CORS_ORIGIN=*
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Admin Access
ADMIN_USER=admin
ADMIN_PASSWORD=your_secure_password

# Church Configuration
DEFAULT_CAMPUS_ID=1
ENABLE_MULTI_CHURCH=true

# Email Notifications
EMAIL_USER=your.email@gmail.com
EMAIL_APP_PASSWORD=your_gmail_app_password
ALERT_EMAIL=alerts@yourdomain.com
